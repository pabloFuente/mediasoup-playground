    "mediasoup-client": "^3.7.12",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "prettier": "^3.3.2",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run format"
    }
  }
