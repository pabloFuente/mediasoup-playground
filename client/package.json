{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "npx tsc && npx vite build",
    "preview": "npx vite preview",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check ."
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "husky": "^9.0.11",
    "prettier": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run format"
    }
  }
}
